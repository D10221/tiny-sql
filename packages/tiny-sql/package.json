{
  "name": "@australis/tiny-sql",
"version": "1.0.0-3",
  "description": "meta package",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --ci --env=node",
    "build": "tsc -p .",
    "clean": "rimraf lib",
    "rebuild": "npm run clean && npm run build",    
    "publish": "npm publish --access public"    
  },
  "keywords": [],
  "author": "daniel@australis-technica.com",
  "license": "MIT",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@australis/tiny-sql-batch-script": "1.0.0-3",
    "@australis/tiny-sql-connect": "1.0.0-3",
    "@australis/tiny-sql-connection-config": "1.0.0-3",
    "@australis/tiny-sql-connection-factory": "1.0.0-3",
    "@australis/tiny-sql-connection-string-parse": "1.0.0-3",
    "@australis/tiny-sql-exec-sql": "1.0.0-3",
    "@australis/tiny-sql-params": "1.0.0-3",
    "@australis/tiny-sql-scripts": "1.0.0-3"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/node": "^10.5.2",
    "jest": "22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "22.4.4",
    "ts-node": "^5.0.0",
    "typescript": "2.9.2"
  }
}
