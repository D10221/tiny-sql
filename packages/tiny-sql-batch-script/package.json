{
  "name": "@australis/tiny-sql-batch-script",
  "version": "1.0.0-9",
  "description": "run a sql/script \"GO\" splitted script ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest --ci --env=node",
    "build": "tsc -p .",
    "clean": "rimraf lib",
    "rebuild": "npm run clean && npm run build",
    "publishit": "npm publish --access public"
  },
  "keywords": [],
  "author": "daniel@australis-technica.com",
  "license": "MIT",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/tedious": "^1.8.33",
    "jest": "22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "22.4.4",
    "ts-node": "^5.0.0",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "@australis/create-debug": "latest"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/australis-technica/sql-batch-script.git"
  },
  "bugs": {
    "url": "https://github.com/australis-technica/sql-batch-script/issues"
  },
  "homepage": "https://github.com/australis-technica/sql-batch-script#readme"
}
