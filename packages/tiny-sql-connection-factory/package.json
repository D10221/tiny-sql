{
  "name": "@d10221/tiny-sql-connection-factory",
  "version": "1.0.0-19",
  "description": "create new ms-sql/tedious, connection from connection-config from env key",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --ci --env=node",
    "build": "rimraf lib && tsc -p .",
    "publishit": "npm publish --access public"
  },
  "keywords": [],
  "author": "d10221@protonmail.com.com",
  "license": "MIT",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "tedious": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/tedious": "^2.6.1",
    "jest": "22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "22.4.4",
    "ts-node": "^5.0.0",
    "typescript": "3.1.6"
  }
}