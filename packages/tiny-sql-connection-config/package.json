{
  "name": "@d10221/tiny-sql-connection-config",
  "version": "1.0.0-19",
  "description": "It provides ms-sql/tedious connection config from connection-string from env key.",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "src": "src/index.ts",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --ci --env=node",
    "build": "rimraf dist &&microbundle --modern  -f es,cjs --external tedious",
    "publishit": "npm publish --access public"
  },
  "keywords": [],
  "author": "d10221@protonmail.com.com",
  "license": "MIT",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@d10221/tiny-sql-connection-string-parse": "1.0.0-19"
  },
  "devDependencies": {
    "@types/tedious": "^2.6.1",
    "jest": "22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "22.4.4",
    "ts-node": "^5.0.0",
    "typescript": "3.1.6"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/australis-technica/sql-connection-config.git"
  },
  "bugs": {
    "url": "https://github.com/australis-technica/sql-connection-config/issues"
  },
  "homepage": "https://github.com/australis-technica/sql-connection-config#readme"
