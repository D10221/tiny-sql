{
  "name": "@d10221/tiny-sql",
  "private": false,
  "version": "1.0.0-beta.28",
  "description": "tiny sql mapper for tediousjs",
  "main": "dist/index.js",
  "src": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --ci --env=node",
    "build:ts": "rimraf build &&  tsc -p . ",
    "bundle": "rimraf dist && node_modules/.bin/microbundle -f cjs --external tedious",
    "build": "yarn build:ts && yarn bundle",
    "prepack": "yarn build && yarn test && yarn pretty",
    "publish:it": "yarn pack",
    "release:it": "release-it --no-npm",
    "pretty": "prettier src**/*.*  test/**/* *.json *.js *.md --write"
  },
  "keywords": [
    "typescript",
    "sql",
    "mapper"
  ],
  "author": "d10221@protonmail.com",
  "license": "MIT",
  "dependencies": {
    "tedious": "8.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/jest": "23.1.5",
    "@types/tedious": "4.0.0",
    "debug": "3.1.0",
    "jest": "24.9.0",
    "microbundle": "0.11.0",
    "prettier": "1.19.1",
    "release-it": "^12.4.3",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5"
  },
  "files": [
    "dist/",
    "src/",
    "build/"
  ]
}
