{
  "name": "@d10221/tiny-sql",
  "private": false,
  "version": "1.0.0-beta.29",
  "description": "tiny sql mapper for tediousjs",
  "main": "index.js",
  "module": "index.esnext.js",
  "types": "index.d.ts",
  "src": "src/index.ts",
  "scripts": {
    "test": "jest --ci --env=node",
    "build:cjs": "rimraf dist/cjs &&  tsc -p . ",
    "build:esnext": "rimraf dist/esnext &&  tsc -p tsconfig.esnext.json",
    "build": "rimraf dist && yarn build:cjs && yarn build:esnext",
    "prepack": "yarn build && yarn test && yarn pretty",
    "publish:it": "yarn pack",
    "release:it": "release-it --no-npm",
    "pretty": "prettier src**/*.*  test/**/* *.json *.js *.md --write"
  },
  "keywords": [
    "typescript",
    "sql",
    "mapper"
  ],
  "author": "d10221@protonmail.com",
  "license": "MIT",
  "dependencies": {
    "tedious": "8.0.1"
  },
  "devDependencies": {
    "@types/jest": "23.1.5",
    "@types/tedious": "4.0.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "release-it": "^12.4.3",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5"
  },
  "files": [
    "dist/",
    "src/",
    "build/"
  ]
}
